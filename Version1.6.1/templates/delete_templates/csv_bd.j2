{% for item in csvfile_vars.split("\n") %}
{%   if loop.index !=1 %}
{%     set list = item.split(",") %}

  - name: "{{ list[0]|trim() }}_{{ list[3]|trim() }}-bd"
    tenant: "{{ list[1]|trim() }}"
    vrf: "{{ list[2]|trim() }}"
    state: delete
    validate_certs: no
{%      endif %}
{%  endfor %}
